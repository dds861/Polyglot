<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.english.learnfast">

    <application
        android:name="com.english.learnfast.RoomDatabase.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.english.learnfast.Activities.HelpActivity"
            android:parentActivityName="com.english.learnfast.Activities.ExamActivity" />
        <activity
            android:name="com.english.learnfast.Activities.NewWordsActivity"
            android:parentActivityName="com.english.learnfast.Activities.SecondActivity" />
        <activity
            android:name="com.english.learnfast.Activities.ExamActivity"
            android:parentActivityName="com.english.learnfast.Activities.SecondActivity" />
        <activity
            android:name="com.english.learnfast.Activities.SecondActivity"
            android:parentActivityName="com.english.learnfast.Activities.MainActivity" />
        <activity
            android:name="com.english.learnfast.Activities.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>